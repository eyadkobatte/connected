generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
  shadowDatabaseUrl = env("DATABASE_SHADOW_URL")
}

model Connection {
  connectionId String @id
  userIdOne String
  userIdTwo String
}

model ConnectionLink {
  connectionId String @id
  fromUserId String
  createdAt DateTime @default(now())
}
